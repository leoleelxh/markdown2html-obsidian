# 图床类型：'alioss' | 'qiniu' | 'github'
REACT_APP_IMAGE_BED_TYPE=github

# 阿里云 OSS 配置
REACT_APP_ALIOSS_ACCESS_KEY_ID=your_access_key_id
REACT_APP_ALIOSS_ACCESS_KEY_SECRET=your_access_key_secret
REACT_APP_ALIOSS_BUCKET=your_bucket_name
REACT_APP_ALIOSS_REGION=oss-cn-hangzhou
REACT_APP_ALIOSS_ENDPOINT=https://oss-cn-hangzhou.aliyuncs.com

# 七牛云配置
REACT_APP_QINIU_ACCESS_KEY=your_access_key
REACT_APP_QINIU_SECRET_KEY=your_secret_key
REACT_APP_QINIU_BUCKET=your_bucket_name
REACT_APP_QINIU_DOMAIN=https://your-domain.com

# GitHub 配置
REACT_APP_GITHUB_TOKEN=your_github_token
REACT_APP_GITHUB_OWNER=your_github_username
REACT_APP_GITHUB_REPO=your_repo_name
REACT_APP_GITHUB_BRANCH=main
REACT_APP_GITHUB_PATH=images/

# 图片处理配置
REACT_APP_IMAGE_COMPRESS_ENABLED=true
REACT_APP_IMAGE_MAX_WIDTH=1920
REACT_APP_IMAGE_MAX_HEIGHT=1080
REACT_APP_IMAGE_QUALITY=0.8
